<%= simple_form_for(@student) do |f| %>
  <%= f.error_notification %>
<div class="col-md-4 col-md-offset-4">
  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :middle_name %>
    <%= f.input :id_number %>
    <%= f.input :gender %>
    <%= f.input :address %>
    <%= f.input :course %>
    <%= f.input :year_level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
</div>
<% end %>
